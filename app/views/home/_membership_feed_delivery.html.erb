<% delivery = membership_feed_delivery %>
<% membership = membership_feed_delivery.feed_item.originator %>
<div id='feed_delivery_<%= delivery.id %>'>
	<div><%= avatar membership.user %></div>
	<div><%= profile_link membership.user %>加入了工会<%= guild_link membership.guild %></div>
	<div><%= link_to_remote 'x', :url => feed_delivery_url(delivery), :method => :delete %></div>
	<div><%= ftime delivery.created_at %></div>
</div>
